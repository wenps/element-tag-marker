{
    "name": "vite-element-tag-marker-plugin",
    "version": "1.0.18",
    "description": "A Vite plugin for marking elements with unique tags",
    "keywords": [
        "vite",
        "plugin",
        "element",
        "tag",
        "marker",
        "vite-plugin"
    ],
    "exports": {
        "require": "./dist/index.cjs",
        "default": "./dist/index.mjs"
    },
    "types": "./dist/index.d.ts",
    "files": [
