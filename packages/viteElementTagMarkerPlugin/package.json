    "repository": {
        "type": "git",
        "url": "https://github.com/wenps/element-tag-marker.git"
    },
    "author": "wenps",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/wenps/element-tag-marker/issues"
    },
    "homepage": "https://github.com/wenps/element-tag-marker#readme",
    "dependencies": {
        "@rollup/plugin-babel": "^6.0.4",
        "element-tag-marker-core": "workspace:*",
        "rollup-plugin-terser": "^7.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.23.2",
        "@babel/traverse": "^7.26.4",
        "@rollup/plugin-typescript": "^11.1.5",
        "@types/babel__core": "^7.20.3",
        "@types/node": "^20.3.3",
        "rollup": "^4.3.0",
        "rollup-plugin-dts": "^6.1.0",
